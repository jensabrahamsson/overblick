# Ã–verblick Secrets Configuration
# ================================
# Copy this file to config/secrets/<identity>.yaml for each identity.
# NEVER commit actual secret files to version control!
#
# To encrypt values in place:
#   python -m overblick secrets encrypt
#
# Encrypted values will be Fernet-encrypted and can only be
# decrypted with the key stored in OVERBLICK_SECRET_KEY env var.
#
# Each identity gets its own secrets file under config/secrets/.
# Only include the secrets relevant to that identity's plugins.

# --- Core (used by email_agent plugin) ---
# principal_name: "Your Name"

# --- Moltbook plugin ---
# moltbook_api_key: "CHANGE_ME"
# moltbook_agent_id: "CHANGE_ME"

# --- Telegram plugin / TelegramNotifier capability ---
# telegram_bot_token: "CHANGE_ME"
# telegram_chat_id: "CHANGE_ME"
# telegram_owner_id: "CHANGE_ME"

# --- Gmail capability ---
# gmail_address: "you@gmail.com"
# gmail_app_password: "CHANGE_ME"
# gmail_send_as: "Your Name <you@gmail.com>"

# --- SMTP email capability ---
# smtp_server: "smtp.example.com"
# smtp_port: "587"
# smtp_login: "CHANGE_ME"
# smtp_password: "CHANGE_ME"
# smtp_from_email: "you@example.com"

# --- GitHub plugin ---
# github_token: "ghp_CHANGE_ME"

# --- Matrix plugin ---
# matrix_access_token: "CHANGE_ME"

# --- Discord plugin ---
# discord_bot_token: "CHANGE_ME"

# --- Webhook plugin ---
# webhook_hmac_secret: "CHANGE_ME"

# --- AI Digest plugin ---
# ai_digest_recipient: "you@example.com"
